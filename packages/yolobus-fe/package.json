  "scripts": {
    "clean:build": "rm -rf ./assets/build",
    "start:dev:hmr": "node webpack/webpack-dev-server.js",
    "build:dev:server": "webpack --mode development --config webpack/webpack.server.dev.js --watch",
    "start:dev:server": "nodemon ./index.js --watch assets/build/server/server.js",
    "dev": "run-p clean:build build:generate:config start:dev:hmr build:dev:server start:dev:server",
    "invoke:dev:server": "run-p clean:build build:dev:server start:dev:server",
    "build:prod:client:disableCdn": "webpack --config webpack/webpack.client.prod.js --colors --display-error-details --env.disable-cdn",
    "build:prod:client": "webpack --mode production --config webpack/webpack.client.prod.js --colors --display-error-details",
    "build:prod:server": "webpack --mode production --config webpack/webpack.server.prod.js --colors --display-error-details",
    "build:generate:config": "node ./createConfig.js",
    "prod-server": "cross-env NODE_ENV=production node index.js",
    "prod": "cross-env NODE_ENV=production npm-run-all clean:build build:generate:config build:prod:client build:prod:server",
    "vlint": "eslint -c .eslintrc",
    "vlint:fix": "eslint -c .eslintrc --fix",
    "lint": "eslint -c .eslintrc app",
    "lint:fix": "eslint -c .eslintrc app --fix",
    "jest": "jest --config ./jest.config.js",
    "jest:cov": "jest --config ./jest.config.js --coverage",
    "jest:watch": "npm run jest --config ./jest.config.js -- --watch"
  },
  "betterScripts": {
    "prod-server": {
      "env": {
        "NODE_PATH": "./app",
        "NODE_ENV": "production"
      },
      "command": "node ./index.js"
    }
  },
